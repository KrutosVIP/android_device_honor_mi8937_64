# Camera
genfscon sysfs /camera_fusion_id_back	u:object_r:sysfs_camera_fusion_id:s0
genfscon sysfs /camera_fusion_id_front	u:object_r:sysfs_camera_fusion_id:s0

# DT2W
genfscon proc /sys/dev/dt2w u:object_r:dt2w_ctrl:s0

# FDT
genfscon sysfs /firmware/devicetree/base/model	u:object_r:fdt_xiaomi:s0

# Flashlights
genfscon sysfs /devices/platform/soc/soc:flashlight_back/leds/flashlight	u:object_r:sysfs_leds:s0
genfscon sysfs /devices/platform/soc/soc:flashlight/leds/flashlight-front	u:object_r:sysfs_leds:s0

# Leds
genfscon sysfs /devices/platform/soc/soc:gpio-leds/leds			u:object_r:sysfs_leds:s0
genfscon sysfs /devices/platform/soc/78b6000.i2c/i2c-2/2-0045/leds	u:object_r:sysfs_leds:s0
genfscon sysfs /devices/platform/soc/7af5000.i2c/i2c-5/5-0045/leds	u:object_r:sysfs_leds:s0

# Vibrator
genfscon sysfs /module/qti_haptics	u:object_r:sysfs_qti_haptics:s0
